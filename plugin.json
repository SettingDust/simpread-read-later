{
  "pluginName": "Simpread Read Later",
  "description": "Searching for simpread read later",
  "version": "0.2.0",
  "author": "SettingDust",
  "logo": "logo.png",
  "update": false,
  "preload": "index.js",
  "pluginSetting": {
    "single": true
  },
  "features": [
    {
      "code": "simpread-read-later",
      "explain": "稍后读搜索",
      "cmds": [
        "sr",
        "sir",
        "简悦"
      ]
    },
    {
      "code": "simpread-read-later-config",
      "explain": "简悦配置",
      "cmds": [
        "src",
        "sirc",
        "简悦配置"
      ]
    },
    {
      "code": "simpread-read-later-config-set",
      "explain": "简悦配置设置",
      "cmds": [
        {
          "type": "regex",
          "label": "简悦配置设置",
          "match": "/configPath|useUrlScheme|browser|prefixUrl/"
        }
      ]
    }
  ]
}
